{
  "name": "@tremho/jove-cli",
  "version": "0.6.9-pre-release.144",
  "description": "tbx command line utility for Jove development",
  "main": "build/index.js",
  "bin": {
    "jove": "build/jove.js"
  },
  "scripts": {
    "prepublish": "rm -fr build && npm run build",
    "clean": "rm -fr build node_modules && npm install",
    "build": "run-script-os",
    "check-cd": "chromedriver --version",
    "build:windows": "tsc",
    "build:nix": "tsc && chmod +x build/jove.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tbTest-build": "tsc; p=`pwd`; cd ../tbTest; node ${p}/build/jove.js build",
    "tbTest-nativescript": "tsc; p=`pwd`; cd ../tbTest; node ${p}/build/jove.js nativescript",
    "sb-mad-build": "tsc; p=`pwd`; cd ../sb-mad; node ${p}/build/jove.js build",
    "sb-mad-nativescript": "tsc; p=`pwd`; cd ../sb-mad; node ${p}/build/jove.js nativescript"
  },
  "keywords": [
    "Jove",
    "framework",
    "cli",
    "tbx",
    "build",
    "tool",
    "tools",
    "desktop",
    "mobile",
    "electron",
    "nativescript",
    "windows",
    "macintosh",
    "linux",
    "android",
    "ios"
  ],
  "author": "tremho",
  "license": "UNLICENSED",
  "dependencies": {
    "@riotjs/webpack-loader": "^5.0.0",
    "@types/sass": "^1.16.0",
    "ansi-colors": "^4.1.1",
    "conversion_cargo": "^2.1.21",
    "fork-ts-checker-notifier-webpack-plugin": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "readline-sync": "^1.4.10",
    "riot": "^5.3.3",
    "sass": "^1.32.11",
    "spectron": "^15.0.0",
    "text-spinner": "^0.0.4",
    "ts-loader": "^9.2.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "webpack": "^5.47.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/node": "^16.4.12",
    "run-script-os": "^1.1.6"
  }
}