{
  "name": "@tremho/jove-cli",
  "version": "0.6.9-pre-release.46",
  "description": "tbx command line utility for Jove development",
  "main": "build/index.js",
  "bin": {
    "tbx": "build/tbx.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "clean": "rm -fr build node_modules && npm install",
    "build": "tsc && chmod +x build/tbx.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tbTest-build": "tsc; p=`pwd`; cd ../tbTest; node ${p}/build/tbx.js build",
    "tbTest-nativescript": "tsc; p=`pwd`; cd ../tbTest; node ${p}/build/tbx.js nativescript",
    "sb-mad-build": "tsc; p=`pwd`; cd ../sb-mad; node ${p}/build/tbx.js build",
    "sb-mad-nativescript": "tsc; p=`pwd`; cd ../sb-mad; node ${p}/build/tbx.js nativescript"
  },
  "keywords": [
    "Jove",
    "framework",
    "cli",
    "tbx",
    "build",
    "tool",
    "tools",
    "desktop",
    "mobile",
    "electron",
    "nativescript",
    "windows",
    "macintosh",
    "linux",
    "android",
    "ios"
  ],
  "author": "tremho",
  "license": "UNLICENSED",
  "dependencies": {
    "@riotjs/webpack-loader": "^5.0.0",
    "@types/sass": "^1.16.0",
    "ansi-colors": "^4.1.1",
    "awesome-typescript-loader": "^5.2.1",
    "riot": "^5.3.3",
    "sass": "^1.32.11",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/node": "^16.4.12",
    "webpack": "^5.47.0"
  }
}